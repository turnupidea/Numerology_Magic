<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    android:id="@+id/MobileResult"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:background="@mipmap/landingpage"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar_mobileNumber"
        android:background="@color/colorPrimary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:textColor="@color/textColor"
            android:id="@+id/back_mobileNumber"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:src="@drawable/toolbar_arrowback"/>
        <ImageView
            android:textColor="@color/textColor"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginLeft="25dp"
            android:src="@mipmap/logoname"/>
        <ImageView
            android:id="@+id/mobileNumber_setting"
            android:background="@drawable/navigation_setting"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:layout_gravity="end"/>
        <ImageView
            android:id="@+id/mobileNumber_question"
            android:layout_gravity="end"
            android:layout_marginRight="5dp"
            android:src="@mipmap/whatsappicon"
            android:layout_width="40dp"
            android:layout_height="40dp"    />
    </android.support.v7.widget.Toolbar>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar_mobileNumber">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textSize="16sp"
                        android:text="@string/luckyNumber"
                        android:typeface="serif"
                        android:textStyle="bold"
                        android:textColor="@color/textColor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:fontFamily="@font/font_edittext" />
                    <TextView
                        android:textSize="25pt"
                        android:typeface="serif"
                        android:textStyle="bold"
                        android:textColor="@color/textColor"

                        android:id="@+id/mobileNumber_text"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:maxLength="1"
                        android:fontFamily="@font/font_edittext"
                        android:textAlignment="center"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_marginLeft="45dp"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textSize="16sp"
                        android:text="@string/karmicNumber"
                        android:typeface="serif"
                        android:textStyle="bold"
                        android:textColor="@color/textColor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:fontFamily="@font/font_edittext" />
                    <TextView
                        android:id="@+id/home_text_karmicNo_mobileNumber"
                        android:textSize="25pt"
                        android:typeface="serif"
                        android:textStyle="bold"
                        android:textColor="@color/textColor"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:maxLength="1"
                        android:fontFamily="@font/font_edittext"
                        android:textAlignment="center"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView
                android:layout_gravity="center"
                android:id="@+id/text1_mobileNumber"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:src="@mipmap/mobilenumber"/>
            <EditText
                android:textSize="16sp"
                android:textStyle="bold"
                android:textColor="@color/edittextcolor"
                android:textColorHint="@color/edittextcolor"
                android:layout_gravity="center"
                android:id="@+id/edit1_mobileNumber"
                android:background="@mipmap/dobinput"
                android:layout_width="300dp"
                android:layout_height="45dp"
                android:layout_marginTop="40dp"
                android:hint="@string/mobilenumber1"
                android:maxLength="10"
                android:digits="0123456789"
                android:layout_below="@+id/text1_mobileNumber"
                android:fontFamily="@font/font_edittext"
                android:textAlignment="center"/>
            <Button
                android:layout_gravity="center"
                android:id="@+id/button1_mobileNumber"
                android:background="@mipmap/clickbutton"
                android:layout_width="180dp"
                android:layout_height="35dp"
                android:layout_marginTop="30dp"/>
            <TextView
                android:textSize="15pt"
                android:text="@string/text_result"
                android:typeface="serif"
                android:textStyle="bold"
                android:textColor="@color/textColor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/font_edittext"
                android:textAlignment="center"/>
            <ImageView
                android:id="@+id/mobileResult_displayResult"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp" />
            <TextView
                android:textSize="60pt"
                android:typeface="serif"
                android:textStyle="bold"
                android:textColor="@color/textColor"
                android:id="@+id/mobileNumber_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:maxLength="1"
                android:fontFamily="@font/font_edittext"
                android:textAlignment="center"/>
            <TextView
                android:textSize="10pt"
                android:padding="20dp"
                android:typeface="serif"
                android:textStyle="normal|bold|italic"
                android:textColor="@color/textColor"
                android:id="@+id/mobileNumber_bestday"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/font_edittext"/>
            <TextView
                android:textSize="10pt"
                android:typeface="serif"
                android:textStyle="normal|bold|italic"
                android:textColor="@color/textColor"
                android:id="@+id/mobileNumber_bestmonth"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/font_edittext"/>
            <TextView
                android:textSize="10pt"
                android:typeface="serif"
                android:textStyle="normal|bold|italic"
                android:textColor="@color/textColor"
                android:id="@+id/mobileNumber_besttime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/font_edittext"/>
